{
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "only-arrow-functions": [
        false
      ],
      "member-ordering": [
        false
      ],
      "member-access": [
        false
      ],
      "space-before-function-paren": ["error", {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "always"
      }],
      "quotemark": [
        "single"
      ],
      "ordered-imports": [
        false
      ],
      "object-literal-sort-keys": false,
      "max-classes-per-file": [
        false
      ],
      "no-empty": false,
      "prefer-for-of": false,
      "forin": false
    }
  }
